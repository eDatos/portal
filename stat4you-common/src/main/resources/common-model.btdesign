Application Stat4YouCommon {
    basePackage=com.stat4you.common

    Module common {
		basePackage=com.stat4you.common

    	"Dto for InternationalString"	
    	DataTransferObject InternationalStringDto {
    		gap
    		
    		-List<@LocalisedStringDto> texts;
    	}

       	"Dto for LocalisedString"	
    	DataTransferObject LocalisedStringDto {
    		gap
    		
       		String label;
       		String locale;
    	}
    }
    	
}
