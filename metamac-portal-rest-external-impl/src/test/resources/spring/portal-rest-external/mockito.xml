<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:p="http://www.springframework.org/schema/p"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    
    <bean id="permalinksService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.portal.core.serviceapi.PermalinksService" />
    </bean>

    <bean id="exportService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.portal.core.serviceapi.ExportService" />
    </bean>
    
    <bean id="statisticalResourcesRestExternal" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.siemac.metamac.portal.core.invocation.StatisticalResourcesRestExternalFacade" />
    </bean>
        
</beans>
