<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings version="2.0"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc= "http://java.sun.com/xml/ns/jaxb/xjc" jaxb:extensionBindingPrefixes="xjc">

	<jaxb:globalBindings> 
		<xjc:simple/> 
		<xjc:javaType xmlType="xs:dateTime" adapter="org.siemac.metamac.portal.rest.external.adapter.DateTimeAdapter" name="java.util.Date" />
		<xjc:javaType xmlType="xs:date" adapter="org.siemac.metamac.portal.rest.external.adapter.DateAdapter" name="java.util.Date" />
	</jaxb:globalBindings> 
</jaxb:bindings>