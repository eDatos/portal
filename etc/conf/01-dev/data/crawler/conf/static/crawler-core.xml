<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	
	<comment>Stat4You Crawler Configuration</comment>

    <entry key="stat4you.crawler.droids.maxThreads">5</entry>
	<entry key="stat4you.crawler.droids.queue.maxSize">-1</entry>
	<entry key="stat4you.crawler.droids.protocol.http.force">true</entry>

	<entry key="stat4you.crawler.droids.handler.save.dir">${stat4you.data.url}/crawler/px</entry>
	<entry key="stat4you.crawler.droids.handler.save.includeHost">true</entry>
	<entry key="stat4you.crawler.droids.delay.request">100</entry>

	<!-- Providers Info -->
	<entry key="stat4you.crawler.droids.provider.istac.url">stat4you:dsd:provider:da17545b-8274-47a2-93ed-4546f68c2a1f</entry>
	<!-- Fire at 1:00am on every Sunday of every month -->
	<entry key="stat4you.crawler.droids.provider.istac.cronexp">0 0 1 ? * SUN</entry>
	
	<entry key="stat4you.crawler.droids.provider.ibestat.url">stat4you:dsd:provider:3da4c0e5-54a6-4433-999c-e5e47f2d248c</entry>
	<!-- Fire at 1:00am on every Monday of every month -->
	<entry key="stat4you.crawler.droids.provider.ibestat.cronexp">0 0 1 ? * MON</entry>
	
	<entry key="stat4you.crawler.droids.provider.eustat.url">stat4you:dsd:provider:128fe08c-1249-46db-83c5-de183d8a9571</entry>
	<!-- Fire at 1:00am on every Tuesday of every month -->
	<entry key="stat4you.crawler.droids.provider.eustat.cronexp">0 0 1 ? * TUE</entry>

	<entry key="stat4you.crawler.droids.provider.ine.url">stat4you:dsd:provider:1282f421-b01e-4e86-bcbd-039da9e82560</entry>
	<!-- Fire at 1:00am on every Wednesday of every month -->
	<entry key="stat4you.crawler.droids.provider.ine.cronexp">0 0 1 ? * WED</entry>

</properties>